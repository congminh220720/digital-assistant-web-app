{
    "extends": ["react-app","prettier"],
    "plugins": ["react","prettier"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "arrowParens": "avoid",
                "semi": false,
                "trailingComma": "all",
                "tabWidth": 2,
                "useTabs": false,
                "singleQuote": true,
                "printWidth": 120,
                "jsxSingleQuote": true,
                "endOfLine": "auto"
            }
        ]
    }
}